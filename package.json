{
  "name": "@sergei-dyshel/typescript",
  "version": "1.0.0",
  "description": "Personal library for typescript",
  "exports": {
    "./enum": "./src/enum.ts",
    "./tsconfig.json": "./tsconfig.json",
    "./zod": "./src/zod.ts",
    "./object": "./src/object.ts",
    "./uri": "./src/uri.ts",
    "./pattern": "./src/pattern.ts",
    "./error": "./src/error.ts",
    "./string": "./src/string.ts",
    "./types": "./src/types.ts",
    "./testing": "./src/testing.ts",
    "./deep-merge": "./src/deep-merge.ts"
  },
  "bin": {
    "qcfg-test": "./scripts/qcfg-test",
    "qcfg-test-all": "./scripts/qcfg-test-all"
  },
  "scripts": {
    "watch": "npx tsc --watch --noEmit --incremental",
    "lint": "npx eslint --color --ext .ts src",
    "postinstall": "[[ -f link.config.json ]] && npx link || true",
    "test": "find . -name '*.test.ts' -exec ./scripts/qcfg-test {} ';'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergei-dyshel/qcfg-js-typescript.git"
  },
