  "homepage": "https://github.com/sergei-dyshel/qcfg-js-typescript",
  "prettier": "@sergei-dyshel/prettier-config",
  "devDependencies": {
    "@sergei-dyshel/eslint-config": "*",
    "@sergei-dyshel/prettier-config": "*",
    "link": "^2.1.0",
    "tsx": "^4.10.5"
  },
  "dependencies": {
    "@types/node": "^18.18.6",
    "case-anything": "^2.1.13",
    "ts-enum-util": "^4.0.2",
    "ts-pattern": "^5.1.1",
    "vscode-uri": "^3.0.8",
    "zod": "^3.22.4"
  },
